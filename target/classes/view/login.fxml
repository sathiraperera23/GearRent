<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="lk.ijse.controller.auth.LoginController"
            prefWidth="400" prefHeight="250">

    <VBox spacing="10" layoutX="100" layoutY="50">
        <TextField fx:id="txtUsername" promptText="Username"/>
        <PasswordField fx:id="txtPassword" promptText="Password"/>
        <Button text="Login" onAction="#login"/>
    </VBox>

</AnchorPane>
